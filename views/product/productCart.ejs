    <%- include("../partial/header") %>
<main>
            <div class="small-container cart-page">

                <table>
                    <tr>
                        <th>Producto</th>
                        <th>Cantidad</th>
                        <th>Subtotal</th>
                    </tr>
                    <tr>
                    <td>
    
                        <div class="cart-info">
                            <img src="/images/monopatines/mp-300-Ninebot-KICKSCOOTER-Max.png">
                            <div>
                                <p>NINEBOT KICKSCOOTER</p>
                                <small>$20.000</small>
                                <br>
                                <a href="">Remover</a>
                            </div>
                        </div>
    
                    </td>
                    <td><input type="number" value="1"></td>
                    <td>$20.000</td>
                </tr>
                <tr>
                    <td class="td-div">
    
                        <div class="cart-info">
                            <img src="/images/monopatines/mp-303-ninebot-air-t15.png">
                            <div>
                                <p>NINEBOT AIR T15</p>
                                <small>$20.000</small>
                                <br>
                                <a href="">Remover</a>
                            </div>
                        </div>
    
                    </td>
                    <td><input type="number" value="1"></td>
                    <td>$20.000</td>
                </tr>
                <tr>
                    <td class="td-div">
    
                        <div class="cart-info">
                            <img src="/images/monociclos/mc-406-MONOCICLO-CICLO.png">
                            <div>
                                <p>MONOCICLO GREY</p>
                                <small>$25.000</small>
                                <br>
                                <a href="">Remover</a>
                            </div>
                        </div>
    
                    </td>
                    <td><input type="number" value="1"></td>
                    <td>$25.000</td>
                </tr>
                </table>
    
                <div class="total-price">
    
                    <table>
                        <tr>
                            <td>Subtotal</td>
                            <td>$65.000</td>
                        </tr>
                        <tr>
                            <td>Env√≠o</td>
                            <td>$3.000</td>
                        </tr>
                        <tr>
                            <td>Descuento</td>
                            <td>$2.500</td>
                        </tr>
                        <tr>
                            <td>Total</td>
                            <td>$65.500</td>
                        </tr>
                        <tr>

                            <td><a class="finalizar-compra" href="/">FINALIZAR COMPRA</a></td>

                        </tr>
                    </table>
    
                </div>
    
            </div>
        </main>

