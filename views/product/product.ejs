<html lang="en">
    <%- include("../partial/head")  %> 
<body>
      <%- include("../partial/header") %> 
      <% for (let dato of items){%>
              <section  class="products-category">
                  <article class="container">
                        
                        <div class="img-contain">
                              <img src="/images/<%= dato.categories_id  %>/<%= dato.image  %>" alt="" width="100%">
                        </div>
                        <div class="contain-titles">
                              <h2><%= dato.name   %></h2>
                              <p><%= dato.description   %></p>
                              <div>
                                    <a class="a-h2-nov" href="/products/category/<%= dato.categories_id  %>">Ver e-<%= dato.category  %></a>
                              </div>   
                        </div>
                  </article>
              </section>

              <%}%> 

        </main>
        <%- include("../partial/footer") %> 
</body>
</html>